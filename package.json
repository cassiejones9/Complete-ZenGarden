{
  "name": "style-guide-template",
  "version": "1.0.0",
  "description": "Boilerplate for a StyleGuide template based off of the CodyHouse template",
  "main": "index.js",
  "scripts": {
    "prestart": "rimraf build/*",
    "start": "parcel examples/csszengarden.html --dist-dir build",
    "prebuild": "rimraf dist/*",
    "build": "parcel build --public-url ./ ./examples/csszengarden.html  --out-dir build",
    "watch": "parcel watch examples/csszengarden.html -dist-dir build/"
  },
  "author": "Shane T",
  "license": "GPL-2.0",
  "devDependencies": {
    "parcel": "^2.2.1",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "@parcel/transformer-sass": "^2.2.1",
    "bootstrap": "^5.1.3"
  },
  "keywords": [],
  "directories": {
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cassiejones9/Complete-ZenGarden.git"
  },
  "bugs": {
    "url": "https://github.com/cassiejones9/Complete-ZenGarden/issues"
  },
